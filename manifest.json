{
  "manifest_version": 3,
  "name": "Tweet",
  "description": "Create crypto coins from any X.com post",
  "version": "1.0",
  "icons": {
    "128": "extension_icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://x.com/*",
        "https://tweetsol.fun/*",
        "https://pump.fun/*"
      ],
      "js": ["content.js"]
    }
  ]
}
